direct to=\"/not-found\" />\n        </React.Fragment>\n      )}\n\n      {token && (\n        <React.Fragment>\n          <div className=\"m-5\">\n            <h1> Welcome {props.users.user.name}..! </h1>\n            <h5> Now You Can Add Your Tasks... Here </h5>\n          </div>\n          <Button variant=\"primary\" onClick={handleShow}>\n            New Project\n          </Button>\n\n          <Modal show={show} onHide={handleClose}>\n            <Modal.Header closeButton>\n              <Modal.Title>New Project</Modal.Title>\n            </Modal.Header>\n            <Modal.Body>\n            <form onSubmit={handleSubmit}>\n          <div className=\"mb-3\">\n            <label htmlFor=\"title\" className=\"form-label\">Title</label>\n            <input\n              value={values.title}\n              onChange={handleInputChange}\n              name=\"title\"\n              type=\"text\"\n              placeholder=\"Title\"\n              className=\"form-control\"\n            />\n          </div>\n          <div className=\"mb-3\">\n            <label htmlFor=\"password\" className=\"form-label\">Description</label>\n            <input\n              value={values.description}\n              onChange={handleInputChange}\n              name=\"description\"\n              type=\"text\"\n              placeholder=\"Description\"\n              className=\"form-control\"/>\n          </div>\n          <div className=\"mb-3\">\n              <Button variant=\"primary\" type=\"submit\" onClick={handleClose}>\n                Submit\n              </Button>\n          </div>\n        </form>\n            </Modal.Body>\n          </Modal>\n\n          <div className=\"container col-4 mt-5\">\n            <ul className=\"list-group\">\n                {props.projects.map(project => (\n                    <li className=\"list-group-item\" \n                    key={project._id}\n                    >\n                      <div className=\"float-left\">\n                        <Link to={`/project/${project._id}`}>View</Link>\n                        <p>Title : {project.title} </p>\n                        <p>Description : {project.description} </p>\n                        </div>\n                        <div className=\"text-right\">\n                        <i className=\"fa fa-trash\" aria-hidden=\"true\" \n                        onClick={() => handleRemove(project)} \n                        style={{ cursor: \"pointer\"}}></i>\n                        </div>\n                    </li>\n                ))}\n            </ul>\n          </div>\n        </React.Fragment>\n      )}\n    </div>\n  );\n};\nconst mapStateToProps = (state) => ({\n  users: state.users,\n  projects: state.projects,\n});\n\nexport default connect(mapStateToProps)(AddProject);\n","/home/rails/Documents/backend/client/src/reducers/projectReducer.js",[],"/home/rails/Documents/backend/client/src/components/project.jsx",["92"],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","severity":1,"message":"98","line":1,"column":33,"nodeType":"99","messageId":"100","endLine":1,"endColumn":39},{"ruleId":"97","severity":1,"message":"101","line":1,"column":10,"nodeType":"99","messageId":"100","endLine":1,"endColumn":14},{"ruleId":"97","severity":1,"message":"102","line":1,"column":16,"nodeType":"99","messageId":"100","endLine":1,"endColumn":24},{"ruleId":"97","severity":1,"message":"103","line":8,"column":3,"nodeType":"99","messageId":"100","endLine":8,"endColumn":16},{"ruleId":"104","severity":1,"message":"105","line":23,"column":7,"nodeType":"106","endLine":23,"endColumn":17,"suggestions":"107"},"no-native-reassign",["108"],"no-negated-in-lhs",["109"],"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar","'Link' is defined but never used.","'Redirect' is defined but never used.","'updateProject' is defined but never used.","react-hooks/exhaustive-deps","React Hook useLayoutEffect has a missing dependency